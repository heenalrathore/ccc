#include <stdio.h>
#include <conio.h>
int main()
{
  int choice, value;
  while(1)
  {
  printf("Stack Operation");
  printf("1:Push 2:POP 3:Display 4:Exit");
  scanf("%d",&choice);
  switch(choice)
  {
   Case 1:
   printf("Enter value to push");
   scanf("%d",&value);
   push(value);
   break;
   Case 2:
   value=POP();
   break;
   Case 3;
   display();
   break;
   Case 4;
   exit(0);
   default:
   printf("Invalid Choice");
  }
}
return 0;
}